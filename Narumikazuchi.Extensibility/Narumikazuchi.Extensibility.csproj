<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Narumikazuchi</Authors>
    <Copyright>Â©2021 Narumikazuchi</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/Narumikazuchi/Extensibility</RepositoryUrl>
    <Description>Provides the ability to extend windows applications with AddIns.
For more information visit https://github.com/Narumikazuchi/Extensibility/wiki.</Description>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Version>1.0.0</Version>
    <PackageIcon>Narumikazuchi nuget.png</PackageIcon>
    <PackageProjectUrl>https://github.com/Narumikazuchi/Extensibility/wiki</PackageProjectUrl>
    <PackageTags>Extensibility; Addin; Plugin; Addon; Windows; Process Isolation</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <DocumentationFile>..\..\..\Managed Code Libraries\Custom\net5.0\Narumikazuchi.Extensibility.xml</DocumentationFile>
    <OutputPath>..\..\..\Managed Code Libraries\Custom\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
    <DocumentationFile>..\..\..\Managed Code Libraries\Custom\net5.0\Narumikazuchi.Extensibility.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Narumikazuchi" Version="1.0.0" />
    <PackageReference Include="Narumikazuchi.Collections.Abstract" Version="1.0.0" />
    <PackageReference Include="Narumikazuchi.Extensibility.Core" Version="1.0.0" />
    <PackageReference Include="Narumikazuchi.Serialization.Bytes" Version="1.0.0" />
    <PackageReference Include="Narumikazuchi.Windows.Pipes" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\Narumikazuchi nuget.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
